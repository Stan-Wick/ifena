% Define CityName character array
CityName = 'Istanbul, Turkey';

% Define LatLong row vector
Latitude = 41.0082;
Longitude = 28.9784;
LatLong = single([Latitude, Longitude]);

% Define CityLocation cell array
CityLocation = {CityName, LatLong};

% Display the results
disp(['City Name: ', CityLocation{1}]);
disp(['Latitude: ', num2str(CityLocation{2}(1))]);
disp(['Longitude: ', num2str(CityLocation{2}(2))]);
