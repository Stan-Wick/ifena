    % Replace negative elements with 999
    B = A;
    B(B < 0) = 999;

    % Replace positive elements with their square root
    C = A;
    C(A > 0) = sqrt(A(A > 0));

    % Find elements between -2 and 2, sort them, and store in column vector D
    filtered_elements = A(A > -2 & A < 2);
    D = sort(filtered_elements(:));
end
